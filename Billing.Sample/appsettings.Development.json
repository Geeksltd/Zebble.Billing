{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AWS": {
    "ServiceURL": "http://localhost:8000"
  },
  "ZebbleBilling": {
    "EntityFramework": {
      "ConnectionString": "Database=Billing.Sample; Server=.; Integrated Security=SSPI; MultipleActiveResultSets=True;"
    },
    "DynamoDb": {
      "Profile": "<AWS_PROFILE_NAME>",
      "Region": "<AWS_REGION>"
    },
    "AppStore": {
      "SharedSecret": "<APP_STORE_SHARED_SECRET>",
      "PrivateKey": "<APP_STORE_PRIVATE_KEY>",
      "KeyId": "<APP_STORE_KEY_ID>",
      "IssuerId": "<APP_STORE_ISSUER_ID>",
      "Environment": "<Sandbox | Production>"
    },
    "GooglePlay": {
      "Store": {
        "ProjectId": "<PROJECT_ID>",
        "PrivateKeyId": "<PRIVATE_KEY_ID>",
        "PrivateKey": "<PRIVATE_KEY>",
        "ClientEmail": "<CLIENT_EMAIL>",
        "ClientId": "<CLIENT_ID>"
      },
      "PubSub": {
        "ProjectId": "<PROJECT_ID>",
        "PrivateKeyId": "<PRIVATE_KEY_ID>",
        "PrivateKey": "<PRIVATE_KEY>",
        "ClientEmail": "<CLIENT_EMAIL>",
        "ClientId": "<CLIENT_ID>",
        "SubscriptionId": "<SUBSCRIPTION_ID>"
      }
    },
    "CafeBazaar": {
      "DeveloperApi": {
        "ClientId": "<CLIENT_ID>",
        "ClientSecret": "<CLIENT_SECRET>"
      }
    },
    "Huawei": {
      "PublicKey": "<CLIENT_ID>",
      "Environment": "<Sandbox | Production>",
      "DeveloperApi": {
        "ClientId": "<CLIENT_ID>",
        "ClientSecret": "<CLIENT_SECRET>"
      }
    }
  }
}
